<div class="bg-light my-5 p-5 shadow bg-body-tertiary rounded">
    <div class="shadow-sm p-3 d-flex justify-content-between">
        <h3 class="mb-0">
            Plants list
        </h3>
        <a href="/plants/form" class="d-flex flex-row-reverse"><button type="button" class="btn btn-primary">Add new plant</button></a>
    </div>
    <div class="d-flex flex-row">
        <% plants.forEach((obj, idx) => { %>
            <div class="card mt-4 me-4">
                <div class="card-body">
                    <h5 class="card-title">
                    Species: <%= obj.plant.species %>
                    </h5>
                    <div>
                    Variety: 
                    <%= obj.plant.variety %>
                    </div>
                    <div>
                    Passport number:  
                    <%= obj.plant.passport %>
                    </div>
                </div>
                </div>
            <% }) %>
    </div>
    
   
</div>

